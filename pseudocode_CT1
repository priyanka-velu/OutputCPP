#include <iostream>
#include <string>

using namespace std

int main()
    string firstName, lastName, streetAddress, city, zipCode

    output "Enter first name: "
    input firstName

    output "Enter last name: "
    input lastName

    output "Enter street address: "
    input streetAddress

    output "Enter city: "
    input city

    cout << "Enter zip code: "
    input zipCode

    output "\nInformation for the fictional person:\n"
    output "First Name: " + firstName
    output "Last Name: " + lastName
    output "Street Address: " + streetAddress
    output "City: " + city
    output "Zip Code: " + zipCode

    return 0